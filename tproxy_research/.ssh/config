Host v1a1
  Hostname 10.2.51.10
  User ubuntu
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  ForwardAgent yes

Host v1b1
  Hostname 10.2.51.20
  User ubuntu
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  ForwardAgent yes

Host u2a1
  Hostname 10.2.81.10
  User ubuntu
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  ForwardAgent yes
  ProxyCommand ssh -F .ssh/config v1a1 -W %h:%p
